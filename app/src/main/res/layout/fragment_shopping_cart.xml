<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#F1F0F0"
    tools:context=".NavbarFragment.ShoppingCartFragment">

    <View
        android:id="@+id/rectangle_6"
        android:layout_width="323dp"
        android:layout_height="513dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginBottom="20dp"
        android:background="@drawable/fondohp"
        android:elevation="4dp"
        app:layout_constraintBottom_toTopOf="@+id/btnHistorial"
        tools:layout_editor_absoluteX="44dp" />

    <Button
        android:id="@+id/btnHistorial"
        android:layout_width="313dp"
        android:layout_height="69dp"
        android:layout_marginTop="16dp"
        android:text="Pagar"
        android:textColor="@color/white"
        android:textSize="16dp"
        android:textStyle="bold"
        app:backgroundTint="@color/red"
        app:elevation="2dp"
        app:layout_constraintTop_toBottomOf="@+id/rectangle_6"
        app:strokeColor="@color/white"
        app:strokeWidth="2dp"
        tools:layout_editor_absoluteX="44dp"></Button>

</androidx.constraintlayout.widget.ConstraintLayout>